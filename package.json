{
  "name": "pear-electron",
  "version": "1.0.16",
  "description": "Pear User-Interface Library for Electron",
  "main": "index.js",
  "bin": "bin.js",
  "scripts": {
    "bootstrap": "pear run bin.js",
    "decal": "node scripts/decal.js",
    "bundle": "node scripts/bundle.js",
    "prepare": "npm run decal && npm run bundle",
    "prestage": "npm run bootstrap && npm run prepare"
  },
  "files": [
    "bin.js",
    "boot.bundle",
    "bootstrap.js",
    "index.js",
    "runtime.js"
  ],
  "pear": {
    "runtimes": "pear://0.3043.sfuy4mdst5rqib69z5juz1injhgtmy5g8gwi7hmzw5g8xpjthcry",
    "name": "pear-electron",
    "stage": {
      "skipWarmup": "true",
      "only": [
        "by-arch",
        "prebuilds"
      ]
    }
  },
  "keywords": [
    "pear",
    "runtime",
    "electron",
    "Pear Runtime",
    "peer-to-peer",
    "user-interface",
    "UI",
    "interface"
  ],
  "author": "Holepunch",
  "license": "Apache-2.0",
  "dependencies": {
    "bare-env": "^3.0.0",
    "bare-fs": "^4.0.1",
    "bare-os": "^3.3.0",
    "bare-path": "^3.0.0",
    "bare-subprocess": "^4.0.3",
    "bare-tty": "^4.0.2",
    "corestore": "^6.18.4",
    "hypercore-id-encoding": "^1.3.0",
    "hyperdrive": "^11.13.3",
    "hyperswarm": "^4.8.4",
    "iambus": "^1.0.3",
    "localdrive": "^1.12.1",
    "node-bare-bundle": "^1.4.2",
    "paparam": "^1.6.1",
    "pear-api": "^1.3.14",
    "pear-interface": "^1.0.3",
    "pear-ipc": "^3.2.0",
    "pear-link": "^2.0.6",
    "pear-updater-bootstrap": "github:holepunchto/pear-updater-bootstrap#corestore-option",
    "rache": "^1.0.0",
    "safety-catch": "^1.0.2",
    "script-linker": "^2.5.3",
    "streamx": "^2.20.2",
    "url-file-url": "^1.0.4",
    "which-runtime": "^1.2.1"
  },
  "bundleDependencies": [
    "hypercore-id-encoding",
    "bare-path",
    "bare-fs",
    "bare-os",
    "bare-tty",
    "bare-path",
    "bare-pipe",
    "bare-subprocess",
    "bare-env",
    "paparam",
    "which-runtime",
    "url-file-url",
    "pear-api"
  ],
  "devDependencies": {
    "@fontsource/open-sans": "^5.1.0",
    "graceful-goodbye": "^1.3.2",
    "redhat-overpass-font": "^1.0.0"
  }
}
